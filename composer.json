{
    "name": "ignitionwolf/wolf-api",
    "description": "",
    "keywords": [
        "wolf-api"
    ],
    "homepage": "https://github.com/IgnitionWolf/wolf-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Ignition Wolf",
            "email": "mauricio@ignitionwolf.com",
            "homepage": "https://ignitionwolf.com",
            "role": "Developer"
        }
    ],
    "repositories": [{
        "type": "path",
        "url": "/home/mauricio/projects/laravel-fillable-relations/",
        "symlink": true
    }],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "flugger/laravel-responder": "^3.1.2",
        "spatie/laravel-query-builder": "^3.3",
        "troelskn/laravel-fillable-relations": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5",
        "orchestra/testbench": "^6.9"
    },
    "autoload": {
        "psr-4": {
            "IgnitionWolf\\API\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IgnitionWolf\\API\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "IgnitionWolf\\API\\WolfAPIServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
