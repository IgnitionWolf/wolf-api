{
    "name": "ignitionwolf/wolf-api",
    "description": "",
    "keywords": [
        "wolf-api"
    ],
    "homepage": "https://github.com/IgnitionWolf/wolf-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Ignition Wolf",
            "email": "hello@ignitionwolf.com",
            "homepage": "https://ignitionwolf.com",
            "role": "Developer"
        }
    ],
    "repositories": [{
        "type": "path",
        "url": "/home/mauricio/projects/docs/",
        "symlink": true
    }],
    "require": {
        "php": "^7.3",
        "flugger/laravel-responder": "^3.1.2",
        "ignitionwolf/wolf-api-docs": "dev-master",
        "laravel/scout": "^8.3",
        "nwidart/laravel-modules": "^8.0",
        "silber/bouncer": "1.0.0-rc.9",
        "tymon/jwt-auth": "^1.0",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.2",
        "illuminate/database": "^7.0",
        "illuminate/routing": "^7.0",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.12"
    },
    "autoload": {
        "psr-4": {
            "IgnitionWolf\\API\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IgnitionWolf\\API\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "IgnitionWolf\\API\\WolfAPIServiceProvider"
            ],
            "aliases": {
                "WolfAPI": "IgnitionWolf\\API\\WolfAPIFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
